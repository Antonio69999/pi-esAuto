<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0"> {% block meta %}{% endblock %}
        <title>
            {% block title %}{% endblock %}
        </title>
        <link rel="stylesheet" href="{{ asset('asset/css/home.css') }}">
        <link rel="stylesheet" href="{{ asset('asset/css/fotorama.css') }}">
        {# FAVICONS #}
        <!-- For Android Chrome -->
        <link rel="icon" sizes="192x192" href="{{ asset('asset/favicon_io/android-chrome-192x192.png') }}"/> <link
        rel="icon" sizes="512x512" href="{{ asset('asset/favicon_io/android-chrome-512x512.png') }}"/>

        <!-- For Apple Touch Icon -->
        <link
        rel="apple-touch-icon" href="{{ asset('asset/favicon_io/apple-touch-icon.png') }}"/>

        <!-- For Favicon 16x16 -->
        <link
        rel="icon" type="image/png" sizes="16x16" href="{{ asset('asset/favicon_io/favicon-16x16.png') }}"/>

        <!-- For Favicon 32x32 -->
        <link
        rel="icon" type="image/png" sizes="32x32" href="{{ asset('asset/favicon_io/favicon-32x32.png') }}"/>

        <!-- For Favicon.ico -->
        <link rel="icon" type="image/x-icon" href="{{ asset('asset/favicon_io/favicon.ico') }}"/> {% block stylesheets %}{% endblock %}{% block javascripts %}{% endblock %}
        </head>
        <body>

            {% block body %}{% endblock %}
            <script src="{{ asset('asset/js/time.js') }}"></script>
        </body>
    </html>
